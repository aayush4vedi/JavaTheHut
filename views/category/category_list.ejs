<h1><%= title %></h1>

<% category.forEach((category)=>{ %>
    <h2><%= category.name %></h2>
    <% category.cook.forEach((cook)=>{ %>
        <h2><%= cook.name %></h2>
    <% }) %>
    <% category.dish.forEach((dish)=>{ %>
        <h2><%= dish.name %></h2>
    <% }) %>
    <a href="/category/<%= category._id %>">Details</a>
<% }) %>