<%- include ('../partials/header') %>
<h1><%= title %></h1>

<a href="/fooder/v1/good/create">Create</a>

<% goods.forEach((good)=>{ %>
    <h2><%= good.name %></h2>
    <h2><%= good.linkToPurchase %></h2>
    <h2><%= good.threshold %></h2>
    <h2><%= good.defaultPurchase %></h2>
    <h2><%= good.quantityInStock %></h2>
    <a href="/fooder/v1/good/<%=good._id %>">Details</a>
<% }) %>


