<%- include ('../partials/header') %>
<a href="/fooder/v1/table/create">Create</a>
<h1><%= title %></h1>

<% tables.forEach((table)=>{ %>
    <h2><%= table.name %></h2>
    <h2><%= table.available %></h2>
    <h2><%= table.capacity %></h2>
    <h2><%= table.waiter.waiterID %></h2>
    <a href="/fooder/v1/table/<%= table._id %>">Details</a>
<% }) %>
