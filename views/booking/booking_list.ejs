<%- include ('../partials/header') %>
<a href="/fooder/v1/booking/create">Create</a>
<h1><%= title %></h1>

<% bookings.forEach((booking)=>{ %>
    <h2><%= booking.customer.name %></h2>
    <h2><%= booking.checkInTime %></h2>
    <h2><%= booking.checkOutTime %></h2>
    <h2><%= booking.table.name %></h2>
    <a href="/fooder/v1/booking/<%= booking._id %>">Details</a>
<% }) %>